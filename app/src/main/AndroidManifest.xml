<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sergio.app_inventario" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        //Actividades botones
        <activity
            android:name=".Crear_Equipo"
            android:label="@string/crear" />

        <activity
            android:name=".Buscar_Equipo"
            android:label="@string/buscar" />

        <activity
            android:name=".Modificar_Equipo"
            android:label="@string/modificar" />

        <activity
            android:name=".Reparar_Equipo"
            android:label="@string/reparar" />

        <activity
            android:name=".Eliminar_Equipo"
            android:label="@string/eliminar" />


    </application>

</manifest>
